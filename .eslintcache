},\n}));\n\n\n//80.1vh, 8.3vhh margin-top\n\nexport default function ShowButton(props) {\n\n  const { array, listOpen, setListOpen } = props\n  const classes = useStyles();\n  const theme = useTheme();\n\n\n\n  function handleDrawerToggle() {\n    setListOpen(!listOpen)\n  }\n\n  return (\n    <div className={`${classes.root} ${'show-button-container'}`}>\n      <Button\n        color=\"inherit\"\n        aria-label=\"open drawer\"\n        onClick={handleDrawerToggle}\n        className={`${clsx(listOpen && classes.hide)} ${'icon-button'}`}\n      >\n        SHOW EVENTS\n      </Button>\n      <Button\n        onClick={() => props.zoomOut()}\n        color=\"inherit\"\n        className=\"reset-button\">\n        Zoom Out\n      </Button>\n      <nav className={`${classes.drawer} `} aria-label=\"mailbox folders\">\n        <Drawer\n          className={`${classes.drawer} `}\n          variant=\"persistent\"\n          anchor=\"right\"\n          open={listOpen}\n          classes={{\n            paper: classes.drawerPaper,\n          }}\n        >\n          <div className={classes.drawerHeader}>\n            <IconButton onClick={handleDrawerToggle}>\n              <ChevronRightIcon />\n            </IconButton>\n            <Typography>Current Events</Typography>\n          </div>\n          <Divider />\n          <div className='first-array-container'>\n            < div className='array-container'>\n              {array}\n            </div >\n          </div >\n        </Drawer>\n      </nav>\n    </div>\n  )\n}","/Users/admin/full-time-unemployed-dev/nasa/nasa-project/src/Components/PaleBlueDot.js",[],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":6,"column":10,"nodeType":"86","messageId":"87","endLine":6,"endColumn":18},{"ruleId":"84","severity":1,"message":"88","line":3,"column":45,"nodeType":"86","messageId":"87","endLine":3,"endColumn":54},{"ruleId":"84","severity":1,"message":"89","line